<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <solid android:color="#3390A6D5" />
            <stroke
                android:width="1dp"
                android:color="@color/bg_button_end"
                android:dashGap="4dp"
                android:dashWidth="10dp" />
            <size
                android:width="100dp"
                android:height="100dp" />
        </shape>
    </item>
    <item
        android:bottom="10dp"
        android:drawable="@drawable/ic_plus_grayed"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp" />

</layer-list>